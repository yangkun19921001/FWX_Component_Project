<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.it.yk.contacts_component">

    <application>

        <!-- 在组件集成宿主时, 由于宿主已经声明了 CommonSDK 的公有 ConfigModule, 所以在这里只需要声明业务组件自己的 ConfigModule -->
        <meta-data
            android:name="com.it.yk.contacts_component.app.GlobalConfiguration"
            android:value="ConfigModule" />

        <activity android:name=".mvp.ui.activity.AddFriendsActivity" />
        <activity android:name=".mvp.ui.activity.ContactsDetailsActivity" />
        <activity android:name=".mvp.ui.activity.NewFriendsActivity" />
        <activity android:name=".mvp.ui.activity.PhoneContactsActivity"></activity>
    </application>

</manifest>